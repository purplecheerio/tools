{
  "servicename": "wbash",
  "routes": [
    {
      "route": "/bash",
      "type": "get",
      "configured": true,
      "routeclass": "./wbash.js",
      "routefunction": "RouteGetSomethingForm",
      "description": "Shell User interface",
      "examples": [
        {
          "param": "/bash"
        }
      ]
    },
    {
      "route": "/bash",
      "type": "post",
      "configured": true,
      "routeclass": "./wbash.js",
      "routefunction": "RoutePostSomething",
      "description": "Bash web console",
      "bodyschema":{
        "type":"object",
        "required":["cmd"],
        "properties":{
          "cmd": {
            "description": "bash command",
            "type": "string"
          }
        }
      },
      "examples": [
        {
          "payload": {
            "cmd":"echo hello"
          }
        }
      ]
    }

  ]
}
