    "npm-run-all": "^4.1.3",
    "os-tmpdir": "^1.0.1",
    "pkg-ok": "^2.2.0",
    "preact": "^8.3.0",
    "prettier": "^1.14.2",
    "promise-polyfill": "^8.0.0",
    "rimraf": "^2.6.2",
    "server-ready": "^0.3.1",
